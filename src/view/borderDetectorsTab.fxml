<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import controller.*?>
<?import components.*?>

<fx:root xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	type="Tab" text="BORDER DETECTOR">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0"
			prefWidth="200.0">
			<children>
				<ToolBar prefHeight="38.0" prefWidth="800.0"
					style="-fx-background-color: white;">
					<items>
						<Button fx:id="prewittButton" mnemonicParsing="false" text="PREWITT"
							styleClass="noise-toolbar" />
						<Button fx:id="sobelButton" mnemonicParsing="false" text="SOBEL"
							styleClass="noise-toolbar" />
					</items>
				</ToolBar>
				<SplitPane layoutY="38.0" prefHeight="534.0" prefWidth="800.0"
					dividerPositions="0.8" layoutX="1.0">
					<items>
						<OpenImage fx:id="image" />
						<TabPane layoutX="14.0" layoutY="80.0"
							prefHeight="450.0" prefWidth="362.0" tabClosingPolicy="UNAVAILABLE">
							<tabs>
								<Tab text="Border">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="180.0" prefWidth="200.0">
											<children>
												<SaveImage fx:id="borderImage"/>
											</children>
										</AnchorPane>
									</content>
								</Tab>
								<Tab text="X Mask">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="180.0" prefWidth="200.0">
											<children>
												<SaveImage fx:id="xImage"/>
											</children>
										</AnchorPane>
									</content>
								</Tab>
								<Tab text="Y Mask">
									<content>
										<AnchorPane minHeight="0.0" minWidth="0.0"
											prefHeight="180.0" prefWidth="200.0">
											<children>
												<SaveImage fx:id="yImage"/>
											</children>
										</AnchorPane>
									</content>
								</Tab>
							</tabs>
						</TabPane>
					</items>
				</SplitPane>
			</children>
		</AnchorPane>
	</content>
</fx:root>