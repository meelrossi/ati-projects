<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import controller.*?>
<?import components.view.*?>

<fx:root xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	type="Tab" text="HOUGH">
	<content>
		<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0"
			prefWidth="200.0">
			<children>
				<ToolBar prefHeight="38.0" prefWidth="800.0"
					style="-fx-background-color: white;">
					<items>
						<Button fx:id="linearButton" mnemonicParsing="false" text="LINEAL"
							styleClass="noise-toolbar" />
						<Button fx:id="circularButton" mnemonicParsing="false"
							text="CIRCULAR" styleClass="noise-toolbar" />
					</items>
				</ToolBar>
				<Pane prefHeight="533.0" layoutY="35" prefWidth="800.0"
					xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
					<children>
						<OpenImage fx:id="image" layoutX="30.0" layoutY="100.0" />
						<SaveImage fx:id="resultImage" layoutX="450.0" layoutY="100.0" />
						<TextField fx:id="stepsField" layoutX="30" layoutY="390.0" />
						<TextField fx:id="titaStepsField" layoutX="200" layoutY="390.0" />
						<TextField fx:id="epsilonField" layoutX="30" layoutY="450.0" />
						<Button fx:id="calculateButton" layoutX="200.0" layoutY="450.0"
							mnemonicParsing="false" text="Calculate" />
					</children>
				</Pane>
			</children>
		</AnchorPane>
	</content>
</fx:root>